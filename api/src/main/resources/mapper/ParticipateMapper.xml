<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cnsmash.cspr.api.mapper.ParticipateMapper">

    <!--玩家ID合并表格更新-->
    <update id="updatePlayerId">
        update `participate`
        set player_id = #{mainPlayerId}
        where player_id = #{subPlayerId}
    </update>
</mapper>